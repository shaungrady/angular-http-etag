{
  "name": "angular-http-etag",
  "version": "2.0.0",
  "description": "Angular module allowing easy use of ETags",
  "homepage": "https://github.com/shaungrady/angular-http-etag#readme",
  "author": "Shaun Grady",
  "license": "MIT",
  "main": "./src",
  "scripts": {
    "umd": "browserify -e . -s angularHttpEtag -t [exposify --expose [ --angular=angular ] ] -p derequire/plugin > ./release/angular-http-etag.js",
    "start": "mochify --watch",
    "phantom": "mochify",
    "coverage": "mochify --plugin [ mochify-istanbul --report lcov --dir './coverage' ]",
    "test": "npm run phantom && npm run coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shaungrady/angular-http-etag.git"
  },
  "keywords": [
    "angular",
    "$http",
    "$cacheFactory",
    "etag",
    "cache",
    "interceptor",
    "decorator"
  ],
  "bugs": {
    "url": "https://github.com/shaungrady/angular-http-etag/issues"
  },
  "dependencies": {
    "array-map": "^0.0.0",
    "object-keys": "^1.0.11"
  },
  "peerDependencies": {
    "angular": ">=1.2 <1.6"
  },
  "devDependencies": {
    "angular": "~1.5.7",
    "angular-mocks": "~1.5.7",
    "browserify": "^13.0.1",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "codeclimate-test-reporter": "^0.3.3",
    "derequire": "^2.0.3",
    "exposify": "^0.5.0",
    "mochify": "^2.18.1",
    "mochify-istanbul": "^2.4.1",
    "phantomjs-prebuilt": "^2.1.7"
  },
  "ignore": [
    "**/.*",
    "node_modules",
    "coverage",
    "test"
  ]
}
