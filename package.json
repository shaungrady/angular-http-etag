{
  "name": "angular-http-etag",
  "version": "2.0.1",
  "description": "Angular module allowing easy use of ETags",
  "homepage": "https://github.com/shaungrady/angular-http-etag#readme",
  "author": "Shaun Grady",
  "license": "MIT",
  "main": "./src",
  "scripts": {
    "start": "mochify --watch test/",
    "build": "npm run umd && npm run uglify",
    "umd": "browserify -e . -s angularHttpEtag -t [exposify --expose [ --angular=angular ] ] -p derequire/plugin > ./release/angular-http-etag.js",
    "uglify": "cd ./release && uglifyjs -c -m --source-map angular-http-etag.min.map -- angular-http-etag.js > angular-http-etag.min.js",
    "phantom": "mochify -R spec test/",
    "coverage": "mochify --plugin [ mochify-istanbul --report lcov --dir './coverage' ] test/",
    "test": "npm run phantom && npm run coverage && eslint ./ && npm run saucelabs",
    "saucelabs": "zuul ./test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shaungrady/angular-http-etag.git"
  },
  "keywords": [
    "angular",
    "$http",
    "etag",
    "cache",
    "$cacheFactory",
    "localStorage",
    "sessionStorage",
    "web storage",
    "entity tag",
    "interceptor",
    "decorator",
    "If-None-Match"
  ],
  "bugs": {
    "url": "https://github.com/shaungrady/angular-http-etag/issues"
  },
  "dependencies": {
    "array-map": "^0.0.0",
    "deepcopy": "^0.6.3",
    "object-keys": "^1.0.11"
  },
  "peerDependencies": {
    "angular": ">=1.2 <1.6"
  },
  "devDependencies": {
    "angular": "~1.5.8",
    "angular-mocks": "~1.5.8",
    "browserify": "^13.1.0",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "codeclimate-test-reporter": "^0.3.3",
    "derequire": "^2.0.3",
    "eslint": "^3.2.0",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-promise": "^2.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "exposify": "^0.5.0",
    "mocha": "^3.0.0",
    "mochify": "^2.18.1",
    "mochify-istanbul": "^2.4.1",
    "phantomjs-prebuilt": "^2.1.9",
    "uglify-js": "^2.7.0",
    "zuul": "^3.10.3"
  },
  "ignore": [
    "**/.*",
    "release/*.min.*",
    "node_modules",
    "docs",
    "coverage",
    "test"
  ]
}
